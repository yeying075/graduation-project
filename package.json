{
  "name": "student-employment",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix \"src/**/*.{js,ts,jsx,tsx,html,vue}\"",
    "format": "prettier --write \"src/**/*.{css,scss,js,ts,jsx,tsx,html,vue,astro}\"",
    "prepare": "husky",
    "apijs": "node expand/createJs.js && prettier --write .",
    "apits": "node expand/createTs.js && prettier --write ."
  },
  "dependencies": {
    "axios": "^1.6.8",
    "dayjs": "^1.11.10",
    "element-plus": "^2.6.3",
    "pinia": "^2.1.7",
    "pinia-plugin-persistedstate": "^3.2.1",
    "three": "^0.163.0",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@rushstack/eslint-patch": "^1.3.3",
    "@tsconfig/node20": "^20.1.2",
    "@types/node": "^20.11.28",
    "@types/three": "^0.163.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^8.0.0",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/tsconfig": "^0.5.1",
    "eslint": "^8.49.0",
    "eslint-plugin-vue": "^9.17.0",
    "husky": "^9.0.11",
    "npm-run-all2": "^6.1.2",
    "prettier": "^3.0.3",
    "sass": "^1.72.0",
    "typescript": "~5.4.0",
    "unplugin-auto-import": "^0.17.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.6",
    "vue-tsc": "^2.0.6"
  }
}
